{
  "extends": "../tsconfig.node.json",
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "declaration": false,
    "lib": [
      "es5",
      "dom"
    ],
    "sourceMap": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "./**/*.d.ts",
    "./**/*.ts",
    "./cypress.d.ts"
  ]
}
