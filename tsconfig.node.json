{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "noEmit": true,
    "lib": [
      "es5",
      "dom"
    ],
    "sourceMap": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "./cypress/**",
    "./vite.config.*"
  ]
}
