{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "declaration": false,
    "outDir": "node_modules/.cache/lit-composition",
    "lib": [
      "es5",
      "dom"
    ],
    "sourceMap": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "./cypress",
    "./tests",
    "./vite.config.*",
    "./cypress.config.*"
  ]
}
